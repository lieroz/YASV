{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build: Debug",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/YASV.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Build: Release",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/YASV.csproj",
                "-c",
                "Release"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Build: GPU Debug",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/YASV_GPUDebug.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}
